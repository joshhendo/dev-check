{
  "name": "npm-dev-check",
  "version": "0.0.2",
  "description": "A static analysis tool for NodeJS project to ensure that no dev dependency is imported by production code",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc -p tsconfig.json",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshhendo/npm-dev-check.git"
  },
  "keywords": [
    "devDependencies",
    "dependencies",
    "check",
    "static",
    "analysis"
  ],
  "author": "Josh Henderson (joshhendo@outlook.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshhendo/npm-dev-check/issues"
  },
  "homepage": "https://github.com/joshhendo/npm-dev-check#readme",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "depcheck": "^0.6.11",
    "es6-promisify": "^6.0.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "read-pkg": "^4.0.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/depcheck": "^0.6.0",
    "@types/es6-promisify": "^6.0.0",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.116",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.10.2",
    "typescript": "^3.0.3"
  },
  "bin": {
    "npm-dev-check": "bin/cli.js"
  }
}
